<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info">
  <Appenders>
    <File name="File" fileName="java-automation-framework.log">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} %level [%t] -%C .%M -%m%n"/>
    </File>
  </Appenders>
  <Loggers>
    <Root level="DEBUG">
      <AppenderRef ref="File"/>
    </Root>
  </Loggers>
</Configuration>
